{
	// Place your snippets for javascript here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"ifJS":{
		"prefix":"ifjs",
		"body": [
			"if(${1}){",
				"\t${2}",
			"}"
		],
		"description": "If para JS"
	},

	"elseJS":{
		"prefix":"else",
		"body": [
			"else{",
				"\t${1}",
			"}"
		],
		"description": "else para JS"
	},

	"elseIfJS":{
		"prefix":"elif",
		"body": [
			"else if(${1}){",
				"\t${2}",
			"}"
		],
		"description": "else if para JS"
	},

	"forJS":{
		"prefix":"forjs",
		"body": [
			"for(${1}; ${2}; ${3}){",
				"\t${4}",
			"}"
		],
		"description": "for para JS"
	},

	"forInJS":{
		"prefix":"forin",
		"body": [
			"for(${1} in ${2}){",
				"\t${3}[${4}]",
			"}"
		],
		"description": "for in para JS"
	},

	"whileJS":{
		"prefix":"wjs",
		"body": [
			"while(${1}){",
				"\t${2}",
			"}"
		],
		"description": "while para JS"
	},

	"doJS":{
		"prefix":"dojs",
		"body": [
			"do{",
				"\t${1}",
			"}",
			"while(${2});"
		],
		"description": "doWhile para JS"
	},

	"QuerySelector":{
		"prefix":"qsel",
		"body": [
			"document.querySelector('${1}');"
		],
		"description": "Query selector para JS"
	},

	"AddEvent":{
		"prefix":"adde",
		"body": [
			"${1}.addEventListener('${2}', () => {",
				"\t",
			"});"
		],
		"description": "Query selector para JS"
	}
}